olor: {{page.color}}; min-height: 8vh; justify-content:space-between;\">\n                        <img class=\"float-left mt-2\" @click=\"clickPage('home')\" :src=\"require('@/assets/big_site_logo.png')\" style=\"max-height: 9vh;\">\n                        <p style=\"font-size: 400%\">{{page.text}}</p>\n                        <div style=\"margin-left: auto;\"></div>\n                        <div \n                            height=\"8vh\" \n                            class=\"justify-end align-center d-flex\"\n                            @click=\"clickPage(pages[selectedPageIndex === 0 ?selectedPageIndex : selectedPageIndex - 1 ].name)\"\n                            ><v-icon \n                                class=\"menu-arrow\" \n                                icon=\"mdi-arrow-left\"\n                                ></v-icon>\n                        </div>\n                        <div \n                            height=\"8vh\" \n                            class=\"justify-end align-center d-flex\"\n                            @click=\"clickPage(pages[selectedPageIndex === pages.length - 1 ? selectedPageIndex : selectedPageIndex + 1].name)\"\n                            ><v-icon \n                                class=\"menu-arrow\" \n                                icon=\"mdi-arrow-right\"\n                                ></v-icon>\n                        </div>\n                    \n\n                    </div>\n                    <v-divider></v-divider>\n                    <component :is=\"page.component\"></component>\n\n                </v-window-item>\n            </v-window>\n            <div\n                width=\"100%\"\n                class=\"nav-window\"\n                :style=\"{\n                    'height': nav_hover ? '50vh' : '10vh'\n                }\"\n                @mouseenter=\"nav_hover = true\"\n                @mouseleave=\"nav_hover = false\"\n                >\n                <div \n                    class=\"d-flex ju