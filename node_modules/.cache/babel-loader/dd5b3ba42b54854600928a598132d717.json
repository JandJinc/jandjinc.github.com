lected_page\" \n        :style=\"{\n            'border': 'solid',\n            'background-color': 'rgba(204, 204, 204, 0.6)',\n            'height': '100vh',  //nav_hover ? '50vh' : '90vh'\n            'transition': 'all 0.2s',\n            'overflow-y': 'auto',\n            'transform': calcOffset(menu_on),\n        }\" \n        >\n        <v-window-item\n            v-for=\"(page, index) in pages\"\n            :key=\"index\"    \n            :value=\"page.name\"\n            >\n            <div class=\"d-flex\" :style=\"{'background-color': page.color, 'min-height': '8vh', 'justify-content': 'space-between',} \">\n                <img class=\"float-left mt-2 ml-4\" @click=\"clickPage('home')\" :src=\"require('@/assets/big_site_logo.png')\" style=\"max-height: 9vh;\">\n                <p  style=\"font-size: 400%\">{{page.text}}</p>\n                <div style=\"margin-left: auto;\"></div>\n                <div \n                    height=\"8vh\" \n                    class=\"justify-end align-center d-flex\"\n                    @click=\"clickPage(pages[selectedPageIndex === 0 ?selectedPageIndex : selectedPageIndex - 1 ].name)\"\n                    ><v-icon \n                        class=\"menu-arrow\" \n                        icon=\"mdi-arrow-left\"\n                        ></v-icon>\n                </div>\n                <div \n                    height=\"8vh\" \n                    class=\"justify-end align-center d-flex\"\n                    @click=\"clickPage(pages[selectedPageIndex === pages.length - 1 ? selectedPageIndex : selectedPageIndex + 1].name)\"\n                    ><v-icon \n                        class=\"menu-arrow\" \n                        icon=\"mdi-arrow-right\"\n                        ></v-icon>\n                </div>\n            \n\n            </div>\n            <v-divider></v-divider>\n            <component :is=\"page.component\"></component>\n\n        </v-window-item>\n    </v-window>\n    <div style=\"width: 100%; transform: translateY(-60px)\" class=\"d-flex justify-center\" >\n        <v-icon \n            icon=\"mdi-home\" \n            class=\"menu-button\" \n            size=\"xxx-large\"\n            @click=\"menu_on = !menu_on\" \n            :style=\"{\n                'border-radius': '100%',\n                'border-width': '2px',\n                'border': 'solid',\n                'background': 'black', \n                'rotate': menu_on ? '180deg' : '0deg'\n            }\" \n            color=\"white\"></v-icon>\n    </div>\n    <div\n        width=\"100%\"\n        class=\"nav-window\"\n        :style=\"{\n            'height': nav_hover ? '50vh' : '10vh'\n        }\"\n\n        >\n        <div \n            class=\"d-flex justify-center\"\n            :style=\"{\n                'font-size': '300%',\n                'opacity': nav_hover ? 0 : 1,\n                'overflow': 'hidden',\n                'height':  nav_hover ? '0px' : '250px',\n                // 'transition-delay': '1000ms',\n                // 'transition-property': 'max-height',\n                'transition': 'height 1s, opacity 1s'\n            }\" >\n            Menu's\n        </div>\n        <div>\n            <div id=\"navlinks\">\n                \n                <div class=\"nav-links\">\n                    <div \n                        v-for=\"(page, index) in pages\"\n                        :key=\"index\"\n                        @click=\"clickPage(page.name)\"\n                        :id=\"'nav-link' + (index + 1).toString()\"\n                        >\n                        <h2 class=\"nav-link-title\">{{page.name}}</h2>\n                        <img :src=\"require('@/assets/' + page.photo)\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-1\">{{page.text}}</p>\n                    </div>\n<!-- \n                    <div @click=\"clickPage('home')\" id=\"nav-link1\">\n                        <h2 class=\"nav-link-title\">Home</h2>\n                        <img :src=\"require('@/assets/startphotoai.jpg')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-1\">De home pagina</p>\n                    </div>\n                    \n                    <div @click=\"clickPage('hoe')\" id=\"nav-link2\">\n                        <h2 class=\"nav-link-title\">Hoe Ai</h2>\n                        <img :src=\"require('@/assets/boxcat.jpg')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-2\">Hoe werkt AI</p>\n                    </div>\n                    <div @click=\"clickPage('voorbeelden')\"  id=\"nav-link3\">\n                        <h2 class=\"nav-link-title\">voorbeelden</h2>\n                        <img :src=\"require('@/assets/aivoorbeelden.jpg')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-3\">Voorbeelden van AI</p>\n                    </div>\n                    <div @click=\"clickPage('gevaar')\" id=\"nav-link4\">\n                        <h2 ckass=\"nav-link-title\">gevaar</h2>\n                        <img :src=\"require('@/assets/waaromai.jpg')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-4\">Is ai gevaar</p>\n                    </div>\n                    <div div @click=\"clickPage('wanneer')\" id=\"nav-link5\">\n                        <h2 class=\"nav-link-title\">Wanneer Ai</h2>\n                        <img :src=\"require('@/assets/design.png')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-5\">Wanneer gebruik je AI</p>\n                    </div>\n                    <div @click=\"clickPage('wat')\" id=\"nav-link6\">\n                        <h2 class=\"nav-link-title\">Wat is Ai</h2>\n                        <img :src=\"require('@/assets/design.png')\" alt=\"\" class=\"nav-link-img\" />\n                        <p class=\"nav-link-p-6\">Wat is AI</p>\n                    </div> -->\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n    <!-- </v-col>\n    <v-col cols=\"12\" md=\"2\" sm=\"0\" v-if=\"$vuetify.display.mdAndUp\"></v-col>\n    \n    \n\n    \n</v-row> -->\n\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\n\nimport HomePage from '@/components/Home'\nimport WatAIPage from '@/components/WatAIPage'\nimport GevaarAIPage from '@/components/GevaarAIPage'\nimport WanneerAIPage from '@/components/WanneerAIPage'\nimport HoeAIPage from '@/components/HoeAIPage'\nimport VoorbeeldenPage from '@/components/VoorbeeldenPage'\n\n\nexport default defineComponent({\n    name: 'Home',\n    components: {\n        HomePage,\n        WatAIPage,\n        GevaarAIPage,\n        HoeAIPage,\n        VoorbeeldenPage,\n        WanneerAIPage\n    },\n    data(){\n        return {\n            nav_hover: false,\n            menu_on: false,\n            pages: [\n                {\n                    name: 'home',\n                    component: 'HomePage',\n                    photo: 'startphotoai.jpg',\n                    color: '#264653',\n                    text: 'Home'\n                },\n                {\n                    name: 'wat',\n                    component: 'WatAIPage',\n                    photo: 'design.png',\n                    color: '#2a9d8f',\n                    text: 'Wat is AI?'\n\n                },\n                {\n                    name: 'gevaar',\n                    component: 'GevaarAIPage',\n                    photo: 'waaromai.jpg',\n                    color: '#e9c46a',\n                    text: 'Is AI gevaar?'\n                },\n                {\n                    name: 'wanneer',\n                    component: 'WanneerAIPage',\n                    photo: 'design.png',\n                    color: '#e9c46a',\n                    text: 'Sinds wanneer is er AI?'\n                },\n                {\n                    name: 'hoe',\n                    component: 'HoeAIPage',\n                    photo: 'boxcat.jpg',\n                    color: '#f4a261',\n                    text: 'Hoe werkt AI?'\n                },\n                {\n                    name: 'voorbeelden',\n                    component: 'VoorbeeldenPage',\n                    photo: 'aivoorbeelden.jpg',\n                    color: '#e76f51',\n                    text: 'Voorbeelden van AI'\n                },\n\n            ],\n            selected_page: ''\n        }\n    },\n    computed: {\n        pageName(){\n            return this.$route.params.page_name\n        },\n        selectedPageIndex(){\n            return this.pages.findIndex(e => e.name === this.selected_page)\n        }\n    },\n    methods: {\n        checkUrl(){\n            if (!this.pages.map(e => e.name).includes(this.pageName)){\n                console.log('wrong page', this.pages.map(e => e.name), this.pageName)\n                this.$router.push('/')\n            } else {\n                this.selected_page = this.pageName\n            }\n        },\n        clickPage(page_name){\n            console.log(page_name)\n            this.$router.push(page_name)\n            this.selected_page = page_name\n        },\n        showNav(){\n            document.body.dataset.navactive = document.body.dataset.navactive === \"true\" ? \"false\" : \"true\";\n        },\n        calcOffset(menu_on){\n            return menu_on ? 'translateY(-40%)' : 'translateY(0%)'\n        }\n    },\n    created() {\n        this.checkUrl()\n    },\n    watch: {\n        pageName(){\n            this.checkUrl()\n        },\n        nav_hover (){\n            document.body.dataset.navactive = this.nav_hover\n            \n        }\n    }\n});\n</script>\n\n<style scoped>\n::-webkit-scrollbar {\n    width: 0;  /* Remove scrollbar space */\n    background: transparent;  /* Optional: just make scrollbar invisible */\n}\n/* Optional: show position indicator in red */\n::-webkit-scrollbar-thumb {\n    background: #FF0000;\n}\n.menu-arrow {\n    transition: font-size 0.1s ease;\n    font-size: 400%\n}\n.menu-arrow:hover {\n    transition: font-size 0.1s ease;\n    font-size: 500%\n}\n.menu-button{\n    transition: all 0.2s ease;\n}\n.menu-button:hover{\n    transform: rotate(180deg)\n}\n@import \"./styles.css\"\n</style>"]},"metadata":{},"sourceType":"module"}