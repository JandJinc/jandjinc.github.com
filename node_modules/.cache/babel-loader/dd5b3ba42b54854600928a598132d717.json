\n        console.log('wrong page', this.pages.map(e => e.name), this.pageName);\n        this.$router.push('/');\n      } else {\n        this.selected_page = this.pageName;\n      }\n    },\n    clickPage(page_name) {\n      console.log(page_name);\n      this.$router.push(page_name);\n      this.selected_page = page_name;\n    },\n    showNav() {\n      document.body.dataset.navactive = document.body.dataset.navactive === \"true\" ? \"false\" : \"true\";\n    },\n    calcOffset(menu_on) {\n      return menu_on ? 'translateY(-40%)' : 'translateY(0%)';\n    }\n  },\n  created() {\n    this.checkUrl();\n  },\n  watch: {\n    pageName() {\n      this.checkUrl();\n    },\n    nav_hover() {\n      document.body.dataset.navactive = this.nav_hover;\n    }\n  }\n});","map":{"version":3,"mappings":";AAqJA,SAASA,eAAc,QAAS,KAAK;AAErC,OAAOC,QAAO,MAAO,mBAAkB;AACvC,OAAOC,SAAQ,MAAO,wBAAuB;AAC7C,OAAOC,YAAW,MAAO,2BAA0B;AACnD,OAAOC,aAAY,MAAO,4BAA2B;AACrD,OAAOC,SAAQ,MAAO,wBAAuB;AAC7C,OAAOC,eAAc,MAAO,8BAA6B;AAGzD,eAAeN,eAAe,CAAC;EAC3BO,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACRP,QAAQ;IACRC,SAAS;IACTC,YAAY;IACZE,SAAS;IACTC,eAAe;IACfF;EACJ,CAAC;EACDK,IAAI,GAAE;IACF,OAAO;MACHC,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE,CACH;QACIL,IAAI,EAAE,MAAM;QACZM,SAAS,EAAE,UAAU;QACrBC,KAAK,EAAE,kBAAkB;QACzBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV,CAAC,EACD;QACIT,IAAI,EAAE,KAAK;QACXM,SAAS,EAAE,WAAW;QACtBC,KAAK,EAAE,YAAY;QACnBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MAEV,CAAC,EACD;QACIT,IAAI,EAAE,QAAQ;QACdM,SAAS,EAAE,cAAc;QACzBC,KAAK,EAAE,cAAc;QACrBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV,CAAC,EACD;QACIT,IAAI,EAAE,SAAS;QACfM,SAAS,EAAE,eAAe;QAC1BC,KAAK,EAAE,YAAY;QACnBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV,CAAC,EACD;QACIT,IAAI,EAAE,KAAK;QACXM,SAAS,EAAE,WAAW;QACtBC,KAAK,EAAE,YAAY;QACnBC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV,CAAC,EACD;QACIT,IAAI,EAAE,aAAa;QACnBM,SAAS,EAAE,iBAAiB;QAC5BC,KAAK,EAAE,mBAAmB;QAC1BC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV,CAAC,CAEJ;MACDC,aAAa,EAAE;IACnB;EACJ,CAAC;EACDC,QAAQ,EAAE;IACNC,QAAQ,