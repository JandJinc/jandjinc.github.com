  },\n  computed: {\n    pageName() {\n      return this.$route.params.page_name;\n    }\n  },\n  methods: {\n    checkUrl() {\n      if (!this.pages.map(e => e.name).includes(this.pageName)) {\n        console.log('hi');\n        this.$router.push('/');\n      }\n    },\n    clickPage(page_name) {\n      $router.push(page.name);\n      this.selected_page = page.name;\n    }\n  },\n  created() {\n    this.checkUrl();\n  },\n  watch: {\n    pageName() {\n      this.checkUrl();\n    }\n  }\n});","map":{"version":3,"mappings":";AAkEA,SAASA,eAAc,QAAS,KAAK;AAErC,OAAOC,QAAO,MAAO,mBAAkB;AACvC,OAAOC,SAAQ,MAAO,wBAAuB;AAC7C,OAAOC,YAAW,MAAO,2BAA0B;AACnD,OAAOC,aAAY,MAAO,4BAA2B;AACrD,OAAOC,SAAQ,MAAO,wBAAuB;AAC7C,OAAOC,eAAc,MAAO,8BAA6B;AAEzD,MAAMC,eAAc,GAAIC,QAAQ,CAACC,gBAAgB,CAAC,wBAAwB,CAAC;AAE3EF,eAAe,CAACG,OAAO,CAACC,MAAK,IAAK;EAC9BA,MAAM,CAACC,gBAAgB,CAAC,OAAO,EAAE,MAAM;IACnC,MAAMC,MAAK,GAAIF,MAAM,CAACG,OAAO,CAACC,cAAa,KAAM,MAAK,GAAI,IAAI,CAAC,CAAC;IAEhE,MAAMC,MAAK,GAAIL,MAAM,CAACM,OAAO,CAAC,iBAAiB,CAAC,CAACC,aAAa,CAAC,eAAe,CAAC;IAC/EC,OAAO,CAACC,GAAG,CAACT,MAAM,CAACM,OAAO,CAAC,iBAAiB,CAAC;IAC7CE,OAAO,CAACC,GAAG,CAACJ,MAAM;IAElB,MAAMK,WAAU,GAAIL,MAAM,CAACE,aAAa,CAAC,eAAe,CAAC;IACzD,IAAII,QAAO,GAAI,CAAC,GAAGN,MAAM,CAACO,QAAQ,CAAC,CAACC,OAAO,CAACH,WAAW,IAAIR,MAAM;IACjE,IAAIS,QAAO,GAAI,CAAC,EAAEA,QAAO,GAAIN,MAAM,CAACO,QAAQ,CAACE,MAAK,GAAI,CAAC;IACvD,IAAIH,QAAO,IAAKN,MAAM,CAACO,QAAQ,CAACE,MAAM,EAAEH,QAAO,GAAI,CAAC;IAEpDN,MAAM,CAACO,QAAQ,CAACD,QAAQ,CAAC,CAACR,OAAO,CAACY,MAAK,GAAI,IAAI;IAC/C,OAAOL,WAAW,CAACP,OAAO,CAACY,MAAM;EACrC,CAAC;AACL,CAAC,CAAC;AAEF,SAASC,IAAI,GAAG;EACZnB,QAAQ,CAACoB,IAAI,CAACd,OAAO,CAACe,SAAQ,GAAIrB,QAAQ,CAACoB,IAAI,CAACd,OAAO,CAACe,SAAQ,KAAM,MAAK,GAAI,OAAM,GAAI,MAAM;AACnG;AAEA,eAAe7B,eAAe,CAAC;EAC3B8B,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACR9B,QAAQ;IACRC,SAAS;IACTC,YAAY;IACZE,SAAS;IACTC,eAAe;IACfF;EACJ,CAAC;EACD4B,IAAI,GAAE;IACF,OAAO;MACHC,KAAK,EAAE,CACH;QACIH,IAAI,EAAE,MAAM;QACZI,SAAS,EAAE;MACf,CAAC,EACD;QACIJ,IAAI,EAAE,KAAK;QACXI,SAAS,EAAE;MAEf,CAAC,EACD;QACIJ