      </v-window-item>\n        </v-window>\n        <div class=\"mt-2\"></div>\n        <div style=\"border: solid; background-color: #979797\">\n            <v-btn \n                v-for=\"(page, index) in pages\"\n                :key=\"index\"\n                @click=\"clickPage(page.name)\"\n                > {{page.name}}\n            </v-btn>\n        </div>\n    </div>\n    <nav id=\"navlinks\">\n        \n        <div class=\"nav-links\">\n            <div @click=\"clickPage('home')\" id=\"nav-link1\">\n                <h2 class=\"nav-link-title\">Home</h2>\n                <img src=\"@/assets/startphotoai.jpg\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-1\">De home pagina</p>\n            </div>\n            \n            <div @click=\"clickPage('hoe')\" id=\"nav-link2\">\n                <h2 class=\"nav-link-title\">Hoe Ai</h2>\n                <img src=\"@/assets/boxcat.jpg\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-2\">Hoe werkt AI</p>\n            </div>\n            <div @click=\"clickPage('voorbeelden')\"  id=\"nav-link3\">\n                <h2 class=\"nav-link-title\">voorbeelden</h2>\n                <img src=\"@/assets/aivoorbeelden.jpg\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-3\">Voorbeelden van AI</p>\n            </div>\n            <div @click=\"clickPage('waarom')\" id=\"nav-link4\">\n                <h2 ckass=\"nav-link-title\">Waarom Ai</h2>\n                <img src=\"@/assets/waaromai.jpg\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-4\">Waarom gebruik je AI</p>\n            </div>\n            <div div @click=\"clickPage('wanneer')\" id=\"nav-link5\">\n                <h2 class=\"nav-link-title\">Wanneer Ai</h2>\n                <img src=\"@/assets/design.png\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-5\">Wanneer gebruik je AI</p>\n            </div>\n            <div @click=\"clickPage('wat')\" id=\"nav-link6\">\n                <h2 class=\"nav-link-title\">Wat is Ai</h2>\n                <img src=\"@/assets/design.png\" alt=\"\" class=\"nav-link-img\">\n                <p class=\"nav-link-p-6\">Wat is AI</p>\n            </div>\n        </div>\n</nav>\n</div>\n\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\n\nimport HomePage from '@/components/Home'\nimport WatAIPage from '@/components/WatAIPage'\nimport WaaromAIPage from '@/components/WaaromAIPage'\nimport WanneerAIPage from '@/components/WanneerAIPage'\nimport HoeAIPage from '@/components/HoeAIPage'\nimport VoorbeeldenPage from '@/components/VoorbeeldenPage'\n\nconst buttonsCarousel = document.querySelectorAll(\"[data-carousel-button]\");\n\nbuttonsCarousel.forEach(button => {\n    button.addEventListener(\"click\", () => {\n        const offset = button.dataset.carouselButton === \"next\" ? 1 : -1;\n\n        const slides = button.closest(\"[data-carousel]\").querySelector(\"[data-slides]\");\n        console.log(button.closest(\"[data-carousel]\"))\n        console.log(slides)\n\n        const activeSlide = slides.querySelector(\"[data-active]\");\n        let newIndex = [...slides.children].indexOf(activeSlide) + offset;\n        if (newIndex < 0) newIndex = slides.children.length - 1;\n        if (newIndex >= slides.children.length) newIndex = 0;\n\n        slides.children[newIndex].dataset.active = true;\n        delete activeSlide.dataset.active;\n    })\n});\n\nfunction show() {\n    document.body.dataset.navactive = document.body.dataset.navactive === \"true\" ? \"false\" : \"true\";\n}\n\nexport default defineComponent({\n    name: 'Home',\n    components: {\n        HomePage,\n        WatAIPage,\n        WaaromAIPage,\n        HoeAIPage,\n        VoorbeeldenPage,\n        WanneerAIPage\n    },\n    data(){\n        return {\n            pages: [\n                {\n                    name: 'home',\n                    component: 'HomePage'\n                },\n                {\n                    name: 'Wat',\n                    component: 'WatAIPage'\n\n                },\n                {\n                    name: 'Waarom',\n                    component: 'WaaromAIPage'\n                },\n                {\n                    name: 'Wanneer',\n                    component: 'WanneerAIPage'\n                },\n                {\n                    name: 'Hoe',\n                    component: 'HoeAIPage'\n                },\n                {\n                    name: 'Voorbeelden',\n                    component: 'VoorbeeldenPage'\n                },\n\n            ],\n            selected_page: ''\n        }\n    },\n    computed: {\n        pageName(){\n            return this.$route.params.page_name\n        }\n    },\n    methods: {\n        checkUrl(){\n            if (!this.pages.map(e => e.name).includes(this.pageName)){\n                console.log('hi')\n                this.$router.push('/')\n            }\n        },\n        clickPage(page_name){\n            $router.push(page.name)\n            this.selected_page = page.name\n        }\n    },\n    created() {\n        this.checkUrl()\n    },\n    watch: {\n        pageName(){\n            this.checkUrl()\n        }\n    }\n});\n</script>\n\n<style scoped>\n@import \"./styles.css\"\n</style>"]},"metadata":{},"sourceType":"module"}