</div>\r\n                </div>\r\n            </v-sheet>\r\n            </v-carousel-item>\r\n        </v-carousel>\r\n    </v-card-text>\r\n    <div class=\"d-flex\">\r\n        <div \r\n            @click=\"selected_slide = slides[selectedSlideIndex == 0 ? selectedSlideIndex : selectedSlideIndex - 1]\" \r\n            style=\"height: 100px; width: 100px; background-color: #FFA200\"\r\n            class=\"d-flex justify-start\"\r\n            >\r\n            <v-icon icon=\"mdi-arrow-left\"></v-icon>\r\n        </div>\r\n        <div \r\n            @click=\"selected_slide = slides[selectedSlideIndex == slides.length - 1 ? selectedSlideIndex : selectedSlideIndex + 1]\" \r\n            style=\"height: 100px; width: 100px; background-color: #FFF000\"\r\n            class=\"d-flex justify-end\"\r\n            \r\n            >\r\n            <v-icon class=\"d-flex align-self-start\" icon=\"mdi-arrow-right\"></v-icon>\r\n        </div>\r\n    </div>\r\n\r\n</v-card>\r\n\r\n\r\n\r\n\r\n\r\n\r\n<!-- <v-row>\r\n    <v-col>\r\n        <v-img src=\"@/assets/aivoorbeelden.jpg\"></v-img>\r\n    </v-col>\r\n    <v-col>\r\n        Voorbbeelden\r\n    </v-col>\r\n</v-row> -->\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n    name: 'VoorbeeldenPage',\r\n    data(){\r\n        return {\r\n            colors: [\r\n          'green',\r\n          'secondary',\r\n          'yellow darken-4',\r\n          'red lighten-2',\r\n          'orange darken-1',\r\n        ],\r\n        slides: [\r\n          'First',\r\n          'Second',\r\n          'Third',\r\n          'Fourth',\r\n          'Fifth',\r\n        ],\r\n        selected_slide: 'green'\r\n\r\n        }\r\n    },\r\n    computed: {\r\n        selectedSlideIndex(){\r\n            \r\n            return this.slides.findIndex(e => e == this.selected_slide)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}